package LinkedLists;

public class main {

	public static void main(String[] args) {
		DList lista = new DList();
		DNode no1 = new DNode("1",null,null);
		DNode no2 = new DNode("2",null,null);
		DNode no3 = new DNode("3",null,null);
		
		lista.addFirst(no1);
		lista.addAfter(no1, no2);
		lista.addAfter(no2, no3);
		
		System.out.println(lista.toString());
		System.out.println(lista.getFirst().getNext().getNext().getElement());
		
		
		
	}
	
}
